#include <bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cin>>t;
    while(t--){
        string s;
        cin>>s;
        int flag=0;
        for(int i=1;i<s.length();i++){
            if(s[i]==s[i-1]){
                flag=1;
                break;
            }
        }
        if(flag==1){
            cout<<1<<endl;
        }else{
            cout<<s.length()<<endl;
        }
    }
}
